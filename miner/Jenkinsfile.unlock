/*
  PIPELINE
 */
pipeline {
  agent any

  options {
    buildDiscarder(logRotator(numToKeepStr: '5')) // Keep last 5 builds only
  }

  stages {
    stage("Checkout") {
      steps {
        checkout scm
      }
    }

    stage("Unlocking data files") {
      steps {
        dir("miner") {
          sh """python unlock_all.py"""
        }
      }
    }
  }
}

